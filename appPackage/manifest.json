{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.24/MicrosoftTeams.schema.json",
  "manifestVersion": "1.24",
  "version": "1.7.0",
  "id": "${{TEAMS_APP_ID}}",
  "developer": {
    "name": "Masato Kobayashi",
    "websiteUrl": "https://www.example.com",
    "privacyUrl": "https://www.example.com/privacy",
    "termsOfUseUrl": "https://www.example.com/termofuse"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "トランスクリプト要約の達人",
    "full": "複数エージェントオーケストレーター"
  },
  "description": {
    "short": "複数のエージェントを呼び出して応答するオーケストレーター",
    "full": "このエージェントは、他の専門エージェント（ワーカーエージェント）を呼び出して、ユーザーの質問に包括的に回答します。Connected Agents機能を使用して、複数のエージェントを統合的にオーケストレーションします。"
  },
  "accentColor": "#FFFFFF",
  "composeExtensions": [],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "copilotAgents": {
    "declarativeAgents": [
      {
        "id": "declarativeAgent",
        "file": "declarativeAgent.json"
      }
    ]
  },
  "validDomains": []
}
